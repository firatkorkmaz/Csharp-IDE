<Window x:Class="CsharpIDE.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="1000" Width="1366">
    <Grid HorizontalAlignment="Stretch" Background="{x:Null}">
        <TextBox Height="30" HorizontalAlignment="Left" Margin="22,47,0,0" Name="programPath" VerticalAlignment="Top" Width="620" TextChanged="programPath_TextChanged" FontFamily="Calibri" FontSize="20" />
        <Button Content="Load" Height="30" HorizontalAlignment="Left" Margin="732,47,0,0" Name="programLoad" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" VerticalContentAlignment="Top" Click="loadProgram_Click" />
        <TextBox Height="658" HorizontalAlignment="Left" Margin="22,87,0,0" Name="programText" VerticalAlignment="Top" Width="780" AcceptsReturn="True" AcceptsTab="True" AutoWordSelection="True" FontFamily="Calibri" FontSize="20" VerticalScrollBarVisibility="Visible" HorizontalScrollBarVisibility="Auto" Text="using System;&#xA;using System.Linq;&#xA;using System.Collections.Generic;&#xA;&#xA;namespace Application&#xA;{&#xA;    class Program&#xA;    {&#xA;        static void Main(string[] args)&#xA;        {&#xA;            string data = &quot;McLaren:10;Porche:9;Lamborghini:8\n&quot; +&#xA;            &#x9;&#x9;&quot;Audi:9;Mercedes:8;Volkswagen:7\n&quot; +&#xA;            &#x9;&#x9;&quot;Mazda:8;Nissan:7;Toyota:6&quot;;&#xA;&#xA;            string[] analyzed = data.Split('\n');&#xA;            List&lt;Register&gt; Row = new List&lt;Register&gt;();&#xA;&#xA;            foreach (string item in analyzed)&#xA;            {&#xA;                Row.Add(new Register(item));&#xA;            }&#xA;&#xA;            foreach (var item in Row)&#xA;            {&#xA;                foreach (var car in item.Data)&#xA;                {&#xA;                    Console.WriteLine(car.brand + &quot;: &quot; + car.rate);&#xA;                }&#xA;                Console.WriteLine();&#xA;            }&#xA;&#xA;            Console.Write(&quot;Press any key to continue . . . &quot;);&#xA;            Console.ReadKey();&#xA;        }&#xA;    }&#xA;}&#xA;" />
        <Button Content="Clear" Height="30" HorizontalAlignment="Left" Margin="652,755,0,0" Name="programClear" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" Click="programClear_Click" />
        <Button Content="Exit" Height="30" HorizontalAlignment="Left" Margin="1253,905,0,0" Name="programExit" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" Click="programExit_Click" />
        <TextBox Height="30" HorizontalAlignment="Left" Margin="823,47,0,0" Name="classPath1" VerticalAlignment="Top" Width="340" FontFamily="Calibri" FontSize="20" TextChanged="classPath1_TextChanged" />
        <Button Content="Browse" Height="30" HorizontalAlignment="Left" Margin="1173,47,0,0" Name="classBrowse1" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" Click="classBrowse1_Click" VerticalContentAlignment="Top" />
        <Button Content="Load" Height="30" HorizontalAlignment="Left" Margin="1253,47,0,0" Name="classLoad1" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" Click="classLoad1_Click" VerticalContentAlignment="Top" />
        <TextBox Height="285" HorizontalAlignment="Left" Margin="823,87,0,0" Name="classText1" VerticalAlignment="Top" Width="500" FontFamily="Calibri" FontSize="20" AcceptsReturn="True" AcceptsTab="True" AutoWordSelection="True" VerticalScrollBarVisibility="Visible" HorizontalScrollBarVisibility="Auto" TextChanged="classText1_TextChanged" Text="using System;&#xD;&#xA;using System.Linq;&#xD;&#xA;using System.Collections.Generic;&#xD;&#xA;&#xD;&#xA;namespace Application&#xD;&#xA;{&#xD;&#xA;    class Cars&#xD;&#xA;    {&#xD;&#xA;        public string brand;&#xD;&#xA;        public int rate;&#xD;&#xA;&#xD;&#xA;        public Cars(string Brand, int Rate)&#xD;&#xA;        {&#xD;&#xA;            brand = Brand;&#xD;&#xA;            rate = Rate;&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;" />
        <TextBlock Height="30" HorizontalAlignment="Left" Margin="22,497,0,0" Name="programBlock" Text="" VerticalAlignment="Top" Width="540" FontFamily="Calibri" FontSize="20" FontStyle="Italic" TextAlignment="Left" />
        <TextBlock Height="30" HorizontalAlignment="Left" Margin="823,248,0,0" Name="classBlock1" Text="" VerticalAlignment="Top" Width="260" FontFamily="Calibri" FontSize="20" TextAlignment="Left" FontStyle="Italic" />
        <Button Content="Save" Height="30" HorizontalAlignment="Left" Margin="732,755,0,0" Name="programSave" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" Click="programSave_Click_1" />
        <TextBox Height="29" HorizontalAlignment="Left" Margin="823,421,0,0" Name="classPath2" VerticalAlignment="Top" Width="340" FontFamily="Calibri" FontSize="20" TextChanged="classPath2_TextChanged" />
        <Button Content="Browse" Height="29" HorizontalAlignment="Left" Margin="1173,421,0,0" Name="classBrowse2" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" VerticalContentAlignment="Top" Click="classBrowse2_Click" />
        <Button Content="Load" Height="29" HorizontalAlignment="Left" Margin="1253,421,0,0" Name="classLoad2" VerticalAlignment="Top" Width="70" VerticalContentAlignment="Top" FontFamily="Calibri" FontSize="20" Click="classLoad2_Click" />
        <Button Content="Clear" Height="29" HorizontalAlignment="Left" Margin="1173,382,0,0" Name="classClear1" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" Click="classClear1_Click" VerticalContentAlignment="Top" />
        <Button Content="Save" Height="29" HorizontalAlignment="Left" Margin="1253,382,0,0" Name="classSave1" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" VerticalContentAlignment="Top" Click="classSave1_Click" />
        <TextBox Height="285" HorizontalAlignment="Left" Margin="823,460,0,0" Name="classText2" VerticalAlignment="Top" Width="500" AcceptsReturn="True" AcceptsTab="True" AutoWordSelection="True" FontFamily="Calibri" FontSize="20" VerticalScrollBarVisibility="Visible" HorizontalScrollBarVisibility="Auto" TextChanged="classText2_TextChanged" Text="using System;&#xD;&#xA;using System.Linq;&#xD;&#xA;using System.Collections.Generic;&#xD;&#xA;&#xD;&#xA;namespace Application&#xD;&#xA;{&#xD;&#xA;    class Register&#xD;&#xA;    {&#xD;&#xA;        public List&lt;Cars&gt; Data;&#xD;&#xA;&#xD;&#xA;        public Register(string Row)&#xD;&#xA;        {&#xD;&#xA;            string[] pieces = Row.Split(';');&#xD;&#xA;&#xD;&#xA;            Data = new List&lt;Cars&gt;();&#xD;&#xA;&#xD;&#xA;            foreach (string item in pieces)&#xD;&#xA;            {&#xD;&#xA;                Data.Add(new Cars(item.Split(':')[0], int.Parse(item.Split(':')[1])));&#xD;&#xA;            }&#xD;&#xA;&#xD;&#xA;        }&#xD;&#xA;    }&#xD;&#xA;}&#xD;&#xA;" />
        <TextBlock Height="30" HorizontalAlignment="Left" Margin="823,498,0,0" Name="classBlock2" Text="" VerticalAlignment="Top" Width="260" FontFamily="Calibri" FontSize="20" FontStyle="Italic" />
        <Button Content="Clear" Height="30" HorizontalAlignment="Left" Margin="1173,755,0,0" Name="classClear2" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" VerticalContentAlignment="Top" Click="classClear2_Click" />
        <Button Content="Save" Height="30" HorizontalAlignment="Left" Margin="1253,755,0,0" Name="classSave2" VerticalAlignment="Top" Width="70" VerticalContentAlignment="Top" FontFamily="Calibri" FontSize="20" Click="classSave2_Click" />
        <Button Content="RUN" Height="30" HorizontalAlignment="Left" Margin="1173,905,0,0" Name="executeButton" VerticalAlignment="Top" Width="70" VerticalContentAlignment="Top" FontFamily="Calibri" FontSize="20" Click="executeButton_Click" />
        <TextBox Height="100" HorizontalAlignment="Left" Margin="22,795,0,0" Name="statusText" VerticalAlignment="Top" Width="1301" VerticalScrollBarVisibility="Auto" FontFamily="Calibri" FontSize="20" TextChanged="statusText_TextChanged" />
        <TextBlock Height="29" HorizontalAlignment="Left" Margin="22,906,0,0" Name="outputLabel" Text="Output File:" VerticalAlignment="Top" Width="100" FontFamily="Calibri" FontSize="20" TextAlignment="Center" />
        <TextBox Height="30" HorizontalAlignment="Left" Margin="130,905,0,0" Name="outputText" VerticalAlignment="Top" Width="592" FontFamily="Calibri" FontSize="20" Text=".\Application" HorizontalContentAlignment="Right" />
        <TextBlock Height="29" HorizontalAlignment="Left" Margin="723,906,0,0" Name="extensionLabel" Text=".exe" VerticalAlignment="Top" Width="40" FontFamily="Calibri" FontSize="20" Foreground="#FFAAAAAA" />
        <Button Content="Save" Height="30" HorizontalAlignment="Left" Margin="1093,905,0,0" Name="allSave" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" VerticalContentAlignment="Top" Click="allSave_Click" />
        <TextBlock Height="29" HorizontalAlignment="Left" Margin="767,906,0,0" Name="frameworkLabel" Text="Framework:" VerticalAlignment="Top" Width="100" TextAlignment="Center" FontFamily="Calibri" FontSize="20" />
        <TextBox Height="30" HorizontalAlignment="Left" Margin="873,905,0,0" Name="frameworkText" VerticalAlignment="Top" Width="50" Text="v4.0" FontFamily="Calibri" FontSize="20" />
        <Button Content="Reset" Height="30" HorizontalAlignment="Left" Margin="572,755,0,0" Name="programReset" VerticalAlignment="Top" Width="70" VerticalContentAlignment="Top" FontFamily="Calibri" FontSize="20" Click="programReset_Click" />
        <Button Content="Reset" Height="29" HorizontalAlignment="Left" Margin="1093,382,0,0" Name="classReset1" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" VerticalContentAlignment="Top" Click="classReset1_Click" />
        <Button Content="Reset" Height="30" HorizontalAlignment="Left" Margin="1093,755,0,0" Name="classReset2" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" VerticalContentAlignment="Top" Click="classReset2_Click" />
        <Button Content="Clear" Height="30" HorizontalAlignment="Left" Margin="1013,905,0,0" Name="allClear" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" VerticalContentAlignment="Top" Click="allClear_Click" />
        <Button Content="Reset" Height="30" HorizontalAlignment="Left" Margin="933,905,0,0" Name="allReset" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" VerticalContentAlignment="Top" Click="allReset_Click" />
        <TextBlock Height="30" HorizontalAlignment="Left" Margin="22,0,0,0" Name="programName" Text="C# Console Application IDE" VerticalAlignment="Top" Width="1301" FontFamily="Calibri" FontSize="20" FontStyle="Italic" TextAlignment="Center" />
        <Button Content="Button" Height="30" HorizontalAlignment="Left" Margin="652,47,0,0" Name="programBrowse" VerticalAlignment="Top" Width="70" FontFamily="Calibri" FontSize="20" VerticalContentAlignment="Top" Click="programBrowse_Click" />
    </Grid>
</Window>
